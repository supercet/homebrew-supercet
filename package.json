{
  "name": "homebrew-supercet",
  "type": "module",
  "version": "0.1.9",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --minify --platform=node --outfile=dist/supercet.js",
    "start": "node dist/supercet.js",
    "bundle": "pkg dist/supercet.js --compress GZip --out-path dist/bundle --targets node22-macos-arm64,node22-macos-x64",
    "build-bundle": "npm run build && npm run bundle",
    "tap:update": "ruby scripts/update-formula.rb",
    "tap:test": "brew install --build-from-source ./Formula/supercet.rb",
    "tap:uninstall": "brew uninstall supercet"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.1",
    "hono": "^4.8.12",
    "jsonwebtoken": "^9.0.2",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "esbuild": "^0.25.8",
    "@types/node": "^20.11.17",
    "@yao-pkg/pkg": "^6.6.0",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
